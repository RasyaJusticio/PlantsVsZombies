{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-trailing-spaces": "off",
        "no-multiple-empty-lines": "off",
        "padded-blocks": "off"

    }
}
